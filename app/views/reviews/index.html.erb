<%= link_to 'New review', new_pill_review_path %>



<h1>Listing reviews</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th> User Id </th>
    <th>Pill Id </th>
  </tr>

  <% @reviews.each do |review| %>
    <tr>
      <td><%= review.title %></td>
      <td><%= review.text %></td>
      <td><%= review.user_id %></td>
      <td><%= review.pill_id %></td>

    </tr>
  <% end %>
</table>



<%= link_to "All Pills", pills_path %><br/>